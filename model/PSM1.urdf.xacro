<?xml version="1.0" ?>
<robot name="dvrk_psm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Macros -->
  <xacro:include filename="$(find dvrk_model)/model/psm.urdf.xacro"/>

  <link name="base_link"/>
  <xacro:psm_sca
      prefix="$(arg prefix)"
      parent_link="base_link"
      xyz="-0.25 0.0 0.5" rpy="0.0 0.0 3.1415926"/>
  
</robot>
